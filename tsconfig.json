{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@components": [
        "./components"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@api/*": [
        "./api/*"
      ]
    }
  }
}
